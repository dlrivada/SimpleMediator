{
  "$schema": "https://stryker-mutator.io/schema/stryker.schema.json",
  "stryker-config": {
    "project": "src/SimpleMediator/SimpleMediator.csproj",
    "test-projects": [
      "tests/SimpleMediator.Tests/SimpleMediator.Tests.csproj",
      "tests/SimpleMediator.PropertyTests/SimpleMediator.PropertyTests.csproj",
      "tests/SimpleMediator.ContractTests/SimpleMediator.ContractTests.csproj"
    ],
    "output-path": "artifacts/mutation",
    "reporters": [
      "progress",
      "html",
      "json",
      "cleartext"
    ],
    "concurrency": 1,
    "thresholds": {
      "high": 85,
      "low": 70,
      "break": 60
    }
  }
}
